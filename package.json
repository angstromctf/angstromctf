{
  "name": "angstromctf",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc && npm run build-css && concurrently \"tsc -w\" \"lite-server\" \"npm run watch-css\"",
    "lite": "lite-server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "build-css": "node-sass app/scss/ -o static/css/",
    "watch-css": "node-sass -w app/scss/ -o static/css/"
  },
  "licenses": [
    {
      "type": "GNU GPLv3",
      "url": "https://github.com/angstromctf/djangoctf/blob/master/LICENSE.txt"
    }
  ],
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "@angular/upgrade": "~2.2.0",
    "core-js": "^2.4.1",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25",
    "bootstrap" :"4.0.0-alpha.5",
    "@ng-bootstrap/ng-bootstrap": "1.0.0-alpha.13"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.45",
    "concurrently": "^3.0.0",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.3",
    "node-sass": "^3.13.0"
  }
}
